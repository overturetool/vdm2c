class ExpressionSeq
functions


protected seqHd: () -> bool
seqHd()== hd [1,2] = 1;

protected seqTl: () -> bool
seqTl()== tl [1,2] = [2];

protected seqLen: () -> bool
seqLen()== len [1,2] = 2;


protected seqElems: () -> bool
seqElems()== elems [1,2] = {1,2};

protected seqInds: () -> bool
seqInds()== inds [1,2] = {1,2};

protected seqConc: () -> bool
seqConc()==  [1]^[2] = [1,2];


protected seqReverse: () -> bool
seqReverse()== reverse [1,2] = [2,1];

protected seqIndex: () -> bool
seqIndex()== [1,2](1)=1;

protected seqMod: () -> bool
seqMod()== [1, 2] ++ {1 |-> 5, 2 |-> 10} = [5,10];
  
protected seqEqual: () -> bool
seqEqual()== [1,2] = [1,2];

protected seqInEqual: () -> bool
seqInEqual()== [1] <> [2];
end ExpressionSeq